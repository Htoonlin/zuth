{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {    
      "myformat": {
        "format": "[%(asctime)s|%(levelname)s|%(clientip)s|%(user)s|%(module)s|%(funcName)s|L%(lineno)d|%(threadName)s] %(message)s",
        "datefmt": "%Y-%m-%dT%H:%M:%S%z" ,
        "defaults":{"clientip":"","user":""}        
      }          
    },
    "handlers": {
      "stderr": {
        "class": "logging.StreamHandler",
        "level": "WARNING",
        "formatter": "myformat",
        "stream": "ext://sys.stdout"
      },
      "file": {
        "class": "logging.handlers.RotatingFileHandler",
        "level": "DEBUG",
        "formatter": "myformat",
        "filename": "app/main/templogs/my_log.txt",
        "maxBytes": 300000000,
        "backupCount": 10
      }
    },
    "loggers": {
      "root": {
        "level": "DEBUG",
        "handlers": [
          "stderr",
          "file"
        ]
      }
    }
    
}