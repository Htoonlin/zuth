{
    "version": 1,    
    "disable_existing_loggers": false,
    "formatters": {     
      "json": {
        "()": "logconfig.MyJSONFormatter",
        "fmt_keys": {
          "timestamp": "timestamp",
          "level": "levelname",
          "clientip": "clientip",
          "user": "user",
          "module": "module",
          "function": "funcName",
          "line": "lineno",
          "thread_name": "threadName",   
          "message":  "message"                         
        }        
      }
    },
    "filters": {
      "no_errors": {
        "()": "logconfig.NonErrorFilter"
      }
    },
    "handlers": {
      "stderr": {
        "class": "logging.StreamHandler",
        "level": "WARNING",
        "formatter": "json",
        "stream": "ext://sys.stdout"
      },
      "file": {
        "class": "logging.handlers.RotatingFileHandler",
        "level": "DEBUG",
        "formatter": "json",
        "filename": "backend/app/main/templogs/my_log.jsonl",
        "maxBytes": 300000000,
        "backupCount": 10
      }
    },
    "loggers": {
      "root": {
        "level": "DEBUG",
        "handlers": [
          "stderr",
          "file"
        ]
      }
    }    
}
